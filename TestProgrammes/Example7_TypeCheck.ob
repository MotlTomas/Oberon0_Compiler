MODULE TypeCheckTest;

VAR
    i: INTEGER;
    r: REAL;
    b: BOOLEAN;

BEGIN
    i := 10;
    r := 3.14;
    b := TRUE;
    
    
    i := 2.5;
    
    r := 42;
    
    b := 1;
    
    IF i THEN
        WRITELN("This should not compile")
    END;
    
    i := i + r;
    
    WRITELN(i);
    WRITELN(r)
END TypeCheckTest.
